<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bishe.portal.dao.TbManageInfomationDao">
    <resultMap id="TbManageInformationMap" type="com.bishe.portal.model.mo.TbManageInformation">
        <id column="id" property="id"/>
        <result column="tittle" property="tittle"/>
        <result column="author" property="author"/>
        <result column="edit" property="edit"/>
        <result column="key" property="key"/>
        <result column="origin" property="origin"/>
        <result column="origin_url" property="originUrl"/>
        <result column="belong_column" property="belongColumn"/>
        <result column="information_status" property="informationStatus"/>
        <result column="down_time" property="downTime"/>
        <result column="is_comment" property="isComment"/>
        <result column="comment_begin_time" property="commentBeginTime"/>
        <result column="comment_end_time" property="commentEndTime"/>
        <result column="summary" property="summary"/>
        <result column="text" property="text"/>
        <result column="create_user" property="createUser"/>
    </resultMap>
    <sql id="TableName">
        tb_manage_information
    </sql>
    <sql id="Fields">
        tittle,author,edit,key,origin,origin_url,belong_column,information_status,down_time,is_comment,comment_begin_time,comment_end_time,summary,text,create_user
    </sql>

    <insert id="insertInformationInfo">
        insert INTO <include refid="TableName"/> (
          <include refid="Fields"/>,
          create_at,
          update_at)
        values(
          #{tbManageInformation.tittle},
          #{tbManageInformation.author},
          #{tbManageInformation.edit},
          #{tbManageInformation.key},
          #{tbManageInformation.origin}
          #{tbManageInformation.originUrl}
          #{tbManageInformation.belongColumn}
          #{tbManageInformation.informationStatus}
          #{tbManageInformation.downTime}
          #{tbManageInformation.isComment}
          #{tbManageInformation.commentBeginTime}
          #{tbManageInformation.commentEndTime}
          #{tbManageInformation.summary}
          #{tbManageInformation.text}
          #{tbManageInformation.createUser},
          NOW(),
          NOW()
        )
        create_at,
        update_at
    </insert>
</mapper>